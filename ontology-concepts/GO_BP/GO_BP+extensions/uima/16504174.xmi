<?xml version="1.0" encoding="UTF-8"?><xmi:XMI xmlns:xmi="http://www.omg.org/XMI" xmlns:metadata="http:///edu/ucdenver/ccp/nlp/core/uima/annotation/metadata.ecore" xmlns:mention="http:///edu/ucdenver/ccp/nlp/core/uima/mention.ecore" xmlns:tcas="http:///uima/tcas.ecore" xmlns:cas="http:///uima/cas.ecore" xmlns:assertion="http:///edu/ucdenver/ccp/nlp/core/uima/assertion.ecore" xmlns:annotation="http:///edu/ucdenver/ccp/nlp/core/uima/annotation.ecore" xmi:version="2.0"><cas:NULL xmi:id="0"/><tcas:DocumentAnnotation xmi:id="2" sofa="1" begin="0" end="22834" language="en"/><annotation:CCPDocumentInformation xmi:id="3" documentID="16504174" documentCollectionID="0" documentSize="0" encoding="UTF_8"/><annotation:CCPTextAnnotation xmi:id="4" sofa="1" begin="218" end="227" annotationID="0" documentSectionID="0" numberOfSpans="1" spans="6" owlClass="GO:0010467"/><annotation:CCPTextAnnotation xmi:id="7" sofa="1" begin="412" end="425" annotationID="0" documentSectionID="0" numberOfSpans="1" spans="9" owlClass="GO_PATO_EXT:biological_localization_process_or_quality"/><annotation:CCPTextAnnotation xmi:id="10" sofa="1" begin="429" end="442" annotationID="0" documentSectionID="0" numberOfSpans="1" spans="12" owlClass="GO_PATO_EXT:physiological_process_or_quality"/><annotation:CCPTextAnnotation xmi:id="13" sofa="1" begin="968" end="982" annotationID="0" documentSectionID="0" numberOfSpans="1" spans="15" owlClass="GO_PATO_RO_EXT:developmental_differentiation_process_or_quality"/><annotation:CCPTextAnnotation xmi:id="16" sofa="1" begin="1035" end="1048" annotationID="0" documentSectionID="0" numberOfSpans="1" spans="18" owlClass="GO_PATO_EXT:physiological_process_or_quality"/><annotation:CCPTextAnnotation xmi:id="19" sofa="1" begin="1151" end="1164" annotationID="0" documentSectionID="0" numberOfSpans="1" spans="21" owlClass="GO_SO_EXT:sequence_rearrangement_process"/><annotation:CCPTextAnnotation xmi:id="22" sofa="1" begin="1183" end="1193" annotationID="0" documentSectionID="0" numb